/**
 * generator client {
 * provider = "prisma-client-js"
 * output   = "../src/generated/prisma"
 * }
 */

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Tasks {
  id   String  @id @default(cuid())
  task String
  done Boolean
}
